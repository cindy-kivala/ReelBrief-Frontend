{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://reelbrief-api.onrender.com",
    "VITE_API_URL": "https://reelbrief-api.onrender.com",
    "VITE_APP_NAME": "ReelBrief",
    "VITE_APP_VERSION": "1.0.0",
    "VITE_CLOUDINARY_CLOUD_NAME": "dfs8mtlxs",
    "VITE_CLOUDINARY_UPLOAD_PRESET": "reelbrief_deliverables",
    "VITE_ENV": "production"
  }
}